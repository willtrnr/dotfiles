# http://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
indent_size = 4

[*.py]
indent_size = 4

[*.cs]
indent_size = 4

[*.vb]
indent_size = 4

[*.rs]
indent_size = 4

[*.md]
max_line_length = 88

[BUILD]
indent_size = 4

[WORKSPACE]
indent_size = 4
